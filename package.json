  "scripts": {
    "clean:server": "rimraf server/dist",
    "clean:client": "rimraf client/.next",
    "clean": "npm-run-all clean:client clean:server",
    "build:server": "tsc -p server",
    "build:client": "next build client",
    "build": "npm-run-all build:client build:server",
    "run:server:dev": "tsc-watch -p server --onSuccess \" node .\"",
    "run:server:prod": "tsc -p server && node .",
    "migrate": "tsc -p server && node ./server/dist/server/src/migrate",
    "pretest": "npm-run-all clean:server build:server",
    "test": "lb-mocha --allow-console-logs \"server/dist/server/src/__tests__\"",
    "dev": "npm-run-all build:client run:server:dev",
    "postinstall": "npm run build",
    "start": "node ."
  },
